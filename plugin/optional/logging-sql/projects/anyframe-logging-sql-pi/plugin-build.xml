<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="logging-sql" description="Logging SQL Plugin consists of reference libraries needed for logging SQL using log4jdbc.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-logging-sql-pi</artifactId>
  <version>1.1.1-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="2.0.0&gt;*&gt;1.4.0"/>
  </dependent-plugins>
  <resources>
   <resource dir="src/main/resources" filtered="true" packaged="false">
      <exclude name="**/log4j.xml" merged="true"/>
    </resource>
  </resources>
  <interceptor>
    <class></class>
  </interceptor>
  <samples>false</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>