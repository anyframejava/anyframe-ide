<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="simpleweb-map" description="Simpleweb-Map Plugin consists of sample code using appropriate functions such as common controllers and tag libraries provided in Anyframe for the simplification of web development, and it provides the way for integration of Spring JS. This Plugin especially use java.util.Map type object as Transfer Object.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-simpleweb-map-pi</artifactId>
  <version>1.0.3-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="simpleweb" version="2.0.0&gt;*&gt;=1.0.2"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
      <include name="**/simpleweb/map/**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*simpleweb-map*.xml"/>
    </resource>
    <resource dir="src/test/java" filtered="true" packaged="true">
      <include name="**/simpleweb/map/**/*.java"/>
    </resource>
    <resource dir="src/test/resources" filtered="true" packaged="false">
      <include name="**/*simpleweb-map*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
        <include name="**/simpleweb-map/**/*.*"/>
        <exclude name="**/tiles-views.xml" merged="true"/>
        <exclude name="**/anyframe.jsp" merged="true"/>
    </resource>
    <resource dir="db/scripts" filtered="false" packaged="false">
        <include name="**/simpleweb-map-*.sql"/>
    </resource>
    <resource dir="db/resources" filtered="false" packaged="false">
        <include name="**/*.*"/>
     </resource>
  </resources>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
  <checked>false</checked>
</plugin>