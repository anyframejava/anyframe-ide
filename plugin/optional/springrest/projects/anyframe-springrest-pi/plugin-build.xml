<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="springrest" description="SpringREST Plugin consists of sample code that provides guidelines on how to use the support for building RESTful applications provided in Spring MVC, and libraries needed to use these open sources.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-springrest-pi</artifactId>
  <version>1.1.1-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="query" version="2.0.0&gt;*&gt;1.4.0"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
      <include name="**/springrest/**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*springrest*.xml"/>
    </resource>
    <resource dir="src/test/java" filtered="true" packaged="true">
      <include name="**/springrest/**/*.java"/>
    </resource>
    <resource dir="src/test/resources" filtered="true" packaged="false">
      <include name="**/*springrest*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
      <include name="**/springrest/**/*.*"/>
      <exclude name="**/web.xml" merged="true"/>
      <exclude name="**/anyframe.jsp" merged="true"/>
    </resource>
    <resource dir="db" filtered="false" packaged="false">
      <include name="**/*.xml"/>
    </resource>
  </resources>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
  <checked>false</checked>
</plugin>