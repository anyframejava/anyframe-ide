<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE queryservice PUBLIC "-//ANYFRAME//DTD QUERYSERVICE//EN"
"http://www.anyframejava.org/dtd/anyframe-core-query-mapping-3.2.dtd">
<queryservice>
  	<queries>
  		<query id="createTable" isDynamic="false">
  			<statement>CREATE TABLE TB_NEW_TABLE ( NO NUMBER(4), NAME VARCHAR2(10)) </statement>
  		</query>   	
  		<query id="dropTable" isDynamic="false">
  			<statement>DROP TABLE TB_NEW_TABLE</statement>
  		</query>   	
		<query id="createIndex" isDynamic="false">
			<statement>
				create index IDX_CUSTOMER on TB_CUSTOMER (ADDRESS)
			</statement>
		</query>
		<query id="dropIndex" isDynamic="false">
			<statement>drop index IDX_CUSTOMER</statement>
		</query>  							  			
  	</queries>
</queryservice>

 
