<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">
	
	<bean id="userSupportDaoWithMap" class="org.anyframe.query.dao.UserSupportDaoWithMap">
		<property name="queryService" ref="queryService"/>
	</bean>								
	
	<bean id="userSupportDaoWithResultClass" class="org.anyframe.query.dao.UserSupportDaoWithResultClass">
		<property name="queryService" ref="queryService"/>
	</bean>
	
	<bean id="userSupportDaoForBatchUpdate" class="org.anyframe.query.dao.UserSupportDaoForBatchUpdate">
		<property name="queryService" ref="queryService"/>
	</bean>		
</beans>	
