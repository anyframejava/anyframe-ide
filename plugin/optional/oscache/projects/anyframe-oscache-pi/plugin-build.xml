<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="oscache" description="OSCache Plugin consists of sample code that provides guidelines on how to use OSCache Service developed based on OSCache of opensymphony, and reference libraries needed to use these open sources.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-oscache-pi</artifactId>
  <version>1.6.1-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="query" version="2.0.0&gt;*&gt;1.4.0"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
      <include name="**/oscache/**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*oscache*.xml"/>
    </resource>
    <resource dir="src/test/java" filtered="true" packaged="true">
      <include name="**/oscache/**/*.java"/>
    </resource>
    <resource dir="src/test/resources" filtered="true" packaged="false">
      <include name="**/*oscache*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
       <include name="**/oscache/**/*.*"/>
       <exclude name="**/anyframe.jsp" merged="true"/>
     </resource>
  </resources>
  <samples>true</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>