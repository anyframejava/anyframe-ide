<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="util" description="Util Plugin consists of sample code that provides basic guidelines on how to use utility of Date, Digest, Number, String, Validation and reference libraries needed to use these open sources.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-util-pi</artifactId>
  <version>1.1.1-SNAPSHOT</version>
  <dependent-plugins>
		<dependent-plugin name="core" version="2.0.0&gt;*&gt;1.4.0" />
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
    	<include name="**/util/**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="false" packaged="false">
    	<include name="**/util*.*"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
    	<include name="**/util*/**/*.*"/>
    	<include name="**/util*.*"/>
    	<exclude name="**/anyframe.jsp" merged="true"/>    
    </resource>
  </resources>
  <samples>true</samples>
  <interceptor>
    <class></class>
  </interceptor>
  <essential>false</essential>
</plugin>