<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="simpleweb" description="Simpleweb Plugin consists of reference libraries and configurations needed to use appropriate functions such as common controllers and tag libraries provided in Anyframe for the simplification of web development.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-simpleweb-pi</artifactId>
  <version>1.0.3-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="idgen" version="2.0.0&gt;*&gt;1.4.0"/>
    <dependent-plugin name="query" version="2.0.0&gt;*&gt;1.4.0"/>
    <dependent-plugin name="tiles" version="2.0.0&gt;*&gt;1.0.2"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
       <include name="**/simpleweb/**/*.*"/>
       <include name="**/simpleweb*.xml"/>
       <exclude name="**/web.xml" merged="true"/>
    </resource>
  </resources>
  <samples>true</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>