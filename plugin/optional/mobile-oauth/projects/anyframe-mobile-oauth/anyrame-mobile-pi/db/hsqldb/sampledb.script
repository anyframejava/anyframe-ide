INSERT INTO MOVIES VALUES('MV-00634','\ubf552014','6.87','','posters/20141204_121_14176537244055WOBG_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\ub4dc\ub77c\ub9c8','2014-12-11',91,'','\ubf40\uc580 \uc0b4\uacb0, \ubd89\uc740 \uc785\uc220, \ud130\uc9c8 \ub4ef\ud55c \uac00\uc2b4\uacfc \uc798\ub85d\ud55c \ud5c8\ub9ac, \uadf8\ub9ac\uace0 \ud48d\ub9cc\ud55c \uc5c9\ub369\uc774\uae4c\uc9c0! \uadf8\uc57c\ub9d0\ub85c \uc644\ubcbd\ud55c \uc678\ubaa8\uc640 \ube60\uc9c8 \uac83 \uc5c6\ub294 \ud14c\ud06c\ub2c9\uc73c\ub85c \ub9c8\uc744 \ub0a8\uc790\ub4e4 \ubaa8\ub450\uc758 \uc560\uac04\uc7a5\uc744 \ub179\uc774\ub358 \uc778\ud574. \ubd80\uc871\ud55c \uac83 \uc5c6\uc774 \ud48d\uc871\ud55c \uc0b6\uc744 \uc0b4\ub358 \uadf8\ub140 \uc55e\uc5d0 \u2018\uc0c9\ub2e4\ub978\u2019 \ub77c\uc774\ubc8c\uc774 \ub4f1\uc7a5\ud55c\ub2e4. \uc0b4\ub791\uac70\ub9ac\ub294 \uae08\ubc1c, \uc0c8\ud30c\ub780 \ub208\ub3d9\uc790, \uadf8\ub9ac\uace0 \uae38\uc8fd\ud55c \ud314\ub2e4\ub9ac\ub97c \uac00\uc9c4 \uc6b0\ud06c\ub77c\uc774\ub098 \ucd9c\uc2e0 \uc548\ud611\uc5d0\uac8c \ubaa8\ub4e0 \ub9c8\uc744 \ub0a8\uc790\ub4e4\uc758 \uad00\uc2ec\uc774 \ub2e4 \uc3df\uc544\uc9c0\uc790 \uc778\ud574\ub294 \uc18d\uc774 \ud0c4\ub2e4. \uc774\uc81c \ubaa8\ub450\ub97c \uc0ac\ub85c\uc7a1\uae30 \uc704\ud55c \uadf8\ub140\uc758 \ud55c\uce35 \ub354 \ub178\uace8\uc801\uc778 \uc720\ud639\uc774 \uc2dc\uc791\ub41c\ub2e4.','\uacf5\uc790\uad00','\uae40\uc5f0\uc218, \uae40\uae30\ucc9c')
