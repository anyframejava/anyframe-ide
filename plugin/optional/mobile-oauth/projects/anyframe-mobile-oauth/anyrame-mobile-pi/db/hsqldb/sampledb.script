INSERT INTO MOVIES VALUES('MV-00374','\ubd81\uc744 \uc6b8\ub824\ub77c','10.0','','posters/20140610_33_140235946191357hDU_JPEG_movie_image.jpg','12\uc138 \uad00\ub78c\uac00','\uc11c\ubd80','2014-06-11',101,'','1840\ub144 \ud50c\ub85c\ub9ac\ub2e4\uc5d0\uc11c \uc778\ub514\uc5b8\ub4e4\uc5d0\uac8c \ucc38\ud328\ub97c \ub2f9\ud558\uace0 \uc788\ub358 \ubbf8\uad6d \uad70\uc778\ub4e4. \uadf8\uacf3\uc73c\ub85c \ub9ac\ucc28\ub4dc \ud130\uce20\ub77c\ub294 \ubbf8\uad6d \ud574\ubcd1\uc911\uc704\uac00 \ubc30\ub97c \uac00\uc9c0\ub85c \ud50c\ub85c\ub9ac\ub2e4\uc8fc \ud034\uc2dc \uc640\uc774\uc5b4\ud2b8 \ub300\ub839 \ubc11\uc73c\ub85c \ubc1c\ub839 \ubc1b\uc558\ub2e4. \uadf8\uacf3\uc5d0\uc11c 40\uba85\uc758 \uad70\uc778\ub9cc \uac00\uc9c0\uace0 \uc694\uc0c8\ub97c \ubb34\ub108 \ub728\ub9ac\ub77c\ub294 \ud130\ubb34\ub2c8 \uc5c6\ub294 \ubbf8\uc158\uc744 \ubc1b\ub294\ub2e4. \ud558\uc9c0\ub9cc \uc640\uc774\uc5b4\ud2b8 \ub300\uc7a5\uc758 \uc9c0\ub3c4\ub825\uc744 \ubc14\ud0d5\uc73c\ub85c \uc694\uc0c8\ub97c \ubb34\ub108 \ub728\ub9ac\ub294\ub370 \uc131\uacf5\ud558\uc9c0\ub9cc \ud6c4\uc5d0 \uc778\ub514\uc5b8\ub4e4\uc758 \ucd94\uaca9\uc744 \ubc1b\ub294\ub2e4. \uadf8\uacf3\uc5d0\uc11c \uc640\uc774\uc5b4\ud2b8 \ub300\uc7a5\uc740 \ub9c8\ub09c \uc5ec\uc778\uacfc \uc0ac\ub791\uc5d0 \ube60\uc9c0\uac8c \ub418\uace0 \uad81\uc9c0\uc5d0 \ubab0\ub9b0 \uc640\uc774\uc5b4\ud2b8 \ub300\uc7a5 \uc77c\ud589\uc740 \uc8fd\uc74c\uc758 \ub2aa\uc774\ub77c \ubd88\ub9ac\uc6b0\ub294 \uc5d0\ubc84\uae00\ub808\uc774\uc988\ub85c \ub4e4\uc5b4\uac04\ub2e4. \uac70\uae30\uc11c \ubc18\uc73c\ub85c \ub098\ub25c \uc77c\ud589\uc740 \uc11c\ub85c \ub5a8\uc5b4\uc838 \ub9cc\ub098\uae30\ub85c \ud55c\uc7a5\uc18c\uc5d0 \uac00\uc9c0\ub9cc \uadf8\uacf3\uc5d0\uc11c \ub73b\ubc16\uc758 \uc77c\ub4e4\uc774 \ubc1c\uc0dd\ud55c\ub2e4.','\ub77c\uc6b8 \uc6d4\uc26c','\uac8c\ub9ac \ucfe0\ud37c, \ub9c8\ub9ac \uc54c\ub3c8')
INSERT INTO MOVIES VALUES('MV-00375','\ud50c\ub808\uc774 \ud3ec \ub370\uc2a4','5.5','','posters/20140605_132_1401928740440pvYcP_JPEG_movie_image.jpg','','\uacf5\ud3ec','2014-06-12',87,'','\uc791\uac00 \ub358\uc740 \uc0ac\ub791\ud588\ub358 \uc5ec\uc790\uce5c\uad6c \uce90\uc11c\ub9b0\uc758 \uc8fd\uc74c\uc5d0 \uc5f0\uad00\ub41c \uc0ac\uc7a5 \uc2a4\ud150\ub9ac\uac00 \ucc3e\uc544\uc624\uc790 \uadf8\ub97c \uae30\uc808\uc2dc\ud0a4\uace0 \uadf8\uc758 \uc7a5\uae30\ub97c \ube7c\ub0b8\ub2e4. \uadf8\ub7ec\ub358 \uc911 \ub358\uc758 \ub2f4\ub2f9\uc790\uc778 \uc544\ubc14\uac00 \ucc3e\uc544\uc628\ub2e4. \ub358\uc758 \uc2dc\ub098\ub9ac\uc624\ub97c \uc88b\uc544\ud588\ub358 \uadf8\ub140\ub294 2\ub144 \ub3d9\uc548 \uc2dc\ub098\ub9ac\uc624\ub97c \uc4f0\uc9c0 \uc54a\uc558\ub358 \uadf8\ub97c \uc548\ud0c0\uae4c\uc6cc\ud558\uba70 \ube68\ub9ac \ub2e4\uc74c \uc2dc\ub098\ub9ac\uc624\ub97c \uc4f0\uae30\ub97c \uc885\uc6a9\ud55c\ub2e4. \uadf8\ub7ec\uc790 \ub358\uc740 \uadf8\ub140\uc5d0\uac8c \uc2dc\ub098\ub9ac\uc624\ub97c \uc4f0\uace0 \uc788\ub2e4\uba74\uc11c \uc790\uc2e0\uc774 \uc2a4\ud150\ub9ac\uc5d0\uac8c \ud55c \uc9d3\uc744 \ubcf4\uc5ec\uc900\ub2e4. \uc544\ubc14\ub294 \ucc98\uc74c\uc5d0\ub294 \ub180\ub77c\uc9c0\ub9cc \uae00\uc744 \uc4f0\uae30 \uc704\ud55c \uc5f0\ucd9c\uc774\ub77c\uace0 \uc0dd\uac01\ud558\uba70 \uc774 \ub0b4\uc6a9\uc744 \uc2dc\ub098\ub9ac\uc624\ub85c \uc4f0\ub294 \uac83\uc744 \uc81c\uc548\ud55c\ub2e4. \uc2a4\ud150\ub9ac\ub97c \uc8fd\uc5ec\uc11c\ub294 \uc548\ub41c\ub2e4\ub294 \ub9d0\uc744 \ub0a8\uae30\uace0 \uc544\ubc14\uac00 \ub3cc\uc544\uac00\uc790 \ub358\uc740 \uadf8\ub140\uc758 \ucda9\uace0\uc640\ub294 \ub2ec\ub9ac \uc2a4\ud150\ub9ac\uc758 \ub2e4\ub978 \uc7a5\uae30\ub97c \ube7c\ub0b4\uace0, \uadf8\uac00 \uc4f0\ub294 \uc2dc\ub098\ub9ac\uc624\ub294 \uc810\uc810 \ub354 \uc794\ud639\ud574\uc838\ub9cc \uac00\ub294\ub370\u2026','BC \ud37c\ud2b8\ub2c8','\uc2a4\ud2f0\ube10 \uc81c\ud504\ub9ac\uc988, \uc5d0\uc988\ub77c \ubc84\uc9d5\ud2bc')
INSERT INTO MOVIES VALUES('MV-00376','\ub370\uc774\ube57 \ubcf4\uc704 \ubba4\uc9c1 \ube44\ub514\uc624 \ud2b9\ubcc4\uc804: The Next Day','7.0','','posters/20140527_194_1401172337895KSkco_JPEG_movie_image.jpg','','','2014-06-06',25,'','\uc804\uc138\uacc4 1\uc5b5 3,100\ub9cc \uc7a5\uc774\ub77c\ub294 \uc74c\ubc18 \ud310\ub9e4\uace0 \uae30\ub85d\uc740 \ubb3c\ub860 \uacf5\uc5f0 \uc5ed\uc0ac\uc0c1 \ub9ce\uc740 \ud2f0\ucf13 \uc138\uc77c\uc988 \uae30\ub85d\uc744 \uac08\uc544\uce58\uc6b4 \uc544\ud2f0\uc2a4\ud2b8, 10\ub144 \ub9cc\uc758 \uc0c8 \uc568\ubc94 \u2018The Next Day\u2019\ub85c \ub85d \uc5ed\uc0ac\uc0c1 \uac00\uc7a5 \ud654\ub824\ud55c \uadc0\ud658\uc744 \uc120\ubcf4\uc778 \uae00\ub7a8 \ub85d\uc758 \uc544\uc774\ucf58 \ub370\uc774\ube57 \ubcf4\uc704\ub97c <2014 FILM LIVE>\ub97c \ud1b5\ud574 \uc2a4\ud06c\ub9b0\uc5d0\uc11c \ub9cc\ub09c\ub2e4. \ub370\uc774\ube57 \ubcf4\uc704\ub294 \uc640 \ubca0\ub974\ub098\ub974\ub3c4 \ubca0\ub974\ud1a8\ub8e8\uce58\uc758 \uc5d0 \uc0bd\uc785\ub41c \u2018Space Oddity\u2019\ub97c \ud1b5\ud574 \ud55c\uad6d\uc5d0\uc11c \ub2e4\uc2dc \ud55c \ubc88 \ub300\uc911\ub4e4\uc758 \uc8fc\ubaa9\uc744 \ubc1b\uace0 \uc788\ub294 \uc544\ud2f0\uc2a4\ud2b8\uc774\uae30\ub3c4 \ud558\ub2e4. \uc774\ubc88 \ud2b9\ubcc4 \uc0c1\uc601\uc744 \ud1b5\ud574 \uc18c\uac1c\ud558\ub294 \ubba4\uc9c1\ube44\ub514\uc624\ub294 2013\ub144 \uadf8\uc758 66\uc138 \uc0dd\uc77c\uc5d0 \uae5c\uc9dd \ubc1c\ub9e4\ub418\uc5b4 \uc804\uc138\uacc4\ub97c \ub180\ub77c\uac8c \ud588\ub358 \uc568\ubc94 \u2018The Next day\u2019\uc758 \uc218\ub85d\uace1\uc73c\ub85c \uad6c\uc131\ub41c\ub2e4. \ub9c8\ub9b4\ub9b0 \ub9e8\uc2a8, \ubdd4\uc695 \ub4f1 \uc720\uc218 \uc544\ud2f0\uc2a4\ud2b8\ub4e4\uc758 \ubba4\uc9c1\ube44\ub514\uc624\ub97c \uc791\uc5c5\ud588\uace0, \ub97c \uac10\ub3c5\ud55c \ud50c\ub85c\ub9ac\uc544 \uc2dc\uc9c0\uc2a4\ubaac\ub514\uac00 \uc5f0\ucd9c\uc744 \ub9e1\uc740 \u2018The Stars(Are Out Tonight)\u2019\ub294 \ud2f8\ub2e4 \uc2a4\uc708\ud2bc\uacfc \ub370\uc774\ube57 \ubcf4\uc704 \uc790\uc2e0\uc774 \uc9c1\uc811 \ucd9c\uc5f0\ud574 \ud654\uc81c\ub97c \ubd88\ub7ec\uc77c\uc73c\ud0a8 \uc791\ud488\uc774\ub2e4. \uc601\ud654\ubc30\uc6b0 \uac8c\ub9ac \uc62c\ub4dc\ub9cc, \ub9c8\ub9ac\uc639 \uaf2c\ub760\uc544\ub974\uac00 \ucd9c\uc5f0\ud55c \u2018The Next Day\u2019, \ub370\uc774\ube57 \ubcf4\uc704\uac00 \uc57d\ubb3c \uc911\ub3c5 \uc644\uce58\ub97c \uc704\ud574 \uba38\ubb3c\uba70 \u2018Heroes\u2019 \ub4f1\uc758 \uba85\ubc18\uc744 \ub9cc\ub4e4\uc5c8\ub358 \ubca0\ub97c\ub9b0 \uc2dc\ub300\ub97c \ud68c\uc0c1\ud558\ub294 \u2018Where Are We Now?\u2019\ub3c4 \uc8fc\ubaa9\ud560 \ub9cc\ud558\ub2e4.','\ud50c\ub85c\ub9ac\uc544 \uc2dc\uc9c0\uc2a4\ubaac\ub514','\ud2f8\ub2e4 \uc2a4\uc708\ud2bc, \ub370\uc774\ube44\ub4dc \ubcf4\uc704')
INSERT INTO MOVIES VALUES('MV-00377','\uc528 \uc5c6\ub294 \uc218\ubc15 \uae40\ub300\uc911','9.0','6.84','posters/20140806_87_1407306877355vmEUQ_JPEG_movie_image.jpg','','\ub2e4\ud050\uba58\ud130\ub9ac','2014-06-06',53,'','\uc528 \uc5c6\ub294 \uc218\ubc15 \uae40\ub300\uc911\uc740 \ud64d\ub300\uc55e\uc5d0\uc11c \ud65c\ub3d9 \uc911\uc778 \ube14\ub8e8\uc2a4 \ud3ec\ud06c \uc74c\uc545\uac00\uc774\ub2e4. \uadf8\ub7ec\ub098 \uadf8\ub294 \ud55c \ubc88\ub3c4 \ubba4\uc9c0\uc158 \uc9c0\ub9dd\uc0dd\uc774\uc5c8\ub358 \uc801\uc774 \uc5c6\ub2e4. \uc6b0\uc5f0\ud788 \uae40\ub300\uc911\uc774\ub77c\ub294 \uc774\ub984\uc744 \uac16\uace0 \ud0dc\uc5b4\ub09c \uadf8\uc5d0\uac8c\ub294 \uadf8\uc800 \uc790\uc2e0\uc774 \uc0b4\uba74\uc11c \uacaa\uc5c8\ub358 \uc77c\ub4e4\uc744 \uc774\uc57c\uae30\ud558\uae30 \uc704\ud574 \uae30\ud0c0\uc640 \ud558\ubaa8\ub2c8\uce74\uac00 \ud544\uc694\ud588\uc744 \ubfd0\uc774\ub2e4. \uc528 \uc5c6\ub294 \uc218\ubc15 \uae40\ub300\uc911\uc758 \uc0b6\uacfc \uc74c\uc545\uc5d0 \ub300\ud574 \uc18c\uac1c\ud558\ub294 \ub2e4\ud050\uba58\ud130\ub9ac. \uc528 \uc5c6\ub294 \uc218\ubc15 \uae40\ub300\uc911\uc740 \uc2ac\ud504\uace0 \uc544\ud320\ub358 \uae30\uc5b5\uc744 \uc790\uc2e0\ub9cc\uc758 \uc704\ud2b8\ub85c \ub178\ub798 \uc18d\uc5d0 \ub2f4\uc544 \uc6b0\ub9ac\uc5d0\uac8c \ub4e4\ub824\uc8fc\ub294 \uc74c\uc545\uac00\ub2e4. \ub178\ub798\ub97c \ubd80\ub974\ub294 \ub3d9\uc548 \uc9c0\uc6b0\uace0 \uc2f6\uc5c8\ub358 \uae30\uc5b5\ub4e4\uc740 \uc5b4\ub290\uc0c8 \uc9c0\uae08\uc758 \uadf8\uc5d0\uac8c \uc790\uc5f0\uc2a4\ub808 \ub179\uc544\ub4e4\uc5b4 \uac04\ub2e4. \ub9e4\uc77c \uc790\uc2e0\uc758 \uacfc\uac70\ub97c \ub300\uba74\ud558\uba70 \ubb34\ub300\uc5d0 \uc624\ub974\ub294 \uae40\ub300\uc911\uc758 \ube14\ub8e8\uc2a4 \uc120\uc728\uc774 \uc9c4\ud55c \uc704\ub85c\ub85c \ub2e4\uac00\uc624\ub294 \uc791\ud488.','\uc774\uc8fc\ud638','\uae40\ub300\uc911')
