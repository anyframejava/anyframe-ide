INSERT INTO MOVIES VALUES('MV-00087','\ud0c0\ub77d\ud55c \uc5ec\uc790','5.67','','posters/20141105_234_1415171294430xOe1g_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\uc561\uc158','2014-11-20',73,'','\uc220\uc9d1\uc5d0 \ub2e4\ub2c8\ub294 \ubbf8\uc0ac\ud0a4\ub294 \ube5a\uc5d0 \ud5c8\ub355\uc774\ub294 \ud0d3\uc5d0 \uc190\ub2d8\ub4e4\ud55c\ud14c \ub3c8\uc744 \ube4c\ub824\ub2ec\ub77c\uace0 \uc870\ub974\ub2e4\uac00 \uc810\uc7a5\ud55c\ud14c \ub4e4\ud0a4\uace0 \uacb0\uad6d \uac00\uac8c\ub97c \uc798\ub9ac\uace0 \ub9cc\ub2e4. \uadf8\ub7ec\ub358 \uc911 \uae38\uc5d0\uc11c \uce5c\ud55c \uc190\ub2d8\uacfc \ub9cc\ub098\uac8c \ub418\ub294\ub370 \ubbf8\uc0ac\ud0a4\ub294 \uadf8 \uc190\ub2d8\ud55c\ud14c\ub3c4 \ub3c8\uc744 \ube4c\ub824\ub2ec\ub77c\uace0 \ud558\uace0, \uadf8 \ub300\uc2e0 \uc190\ub2d8\uacfc \uc220 \ud55c\uc794\uc744 \ud558\ub7ec \uac00\uac8c \ub41c\ub2e4. \uacb0\uad6d \uc220\uc744 \ub9c8\uc2dc\uace0\ub294 \uadf8 \uc190\ub2d8 \uc9d1\uc5d0 \uac19\uc774 \uac00\uac8c \ub418\ub294\ub370, \uadf8 \ub54c \uadf8 \uc9d1\uc744 \ubc29\ubb38\ud55c \ud55c \ub0a8\uc790\uac00 \uc788\uc5c8\uc73c\ub2c8, \uadf8 \ub0a8\uc790\ub294 \uace0\ub4f1\ud559\uad50 \ub3d9\ucc3d\uc778 \ud558\uc57c\ud1a0\uc600\ub2e4. \uadf8\ub0e5 \ubcf4\uc544\ub3c4 \ud3c9\ubc94\ud55c \uc77c\uc744 \ud558\ub294 \uac83 \uac19\uc774\ub294 \ubcf4\uc774\uc9c0 \uc54a\uc558\uc9c0\ub9cc \ud558\uc57c\ud1a0\uc758 \uc0ac\uc7a5\uc774 \uadf8 \uc190\ub2d8\uc744 \ud638\ucd9c\ud55c \uac83\uc774\ub2e4. \ubcc4 \uc758\ubbf8 \uc5c6\uc774 \ub530\ub77c\ub098\uc120 \ubbf8\uc0ac\ud0a4\uc9c0\ub9cc \ub3c8\uc774 \uae09\ud588\ub358 \ubbf8\uc0ac\ud0a4\ub294 \ud559\uad50 \uc120\ubc30\uc600\ub358 \uadf8 \uc0ac\uc7a5\uc758 \ube5a\uc744 \uac1a\uc544\uc8fc\ub294 \ub300\uc2e0 \uc77c\uc744 \ud558\ub77c\ub294 \uc81c\uc758\ub97c \ubc1b\uc544\ub4e4\uc774\ub294\ub370, \uc55e\uc73c\ub85c \ubc8c\uc5b4\uc9c8 \uc77c\uc5d0 \ub300\ud574\uc11c \ubbf8\uc0ac\ud0a4\ub294 \uc804\ud600 \uc544\ub294 \uac8c \uc5c6\uc5c8\uace0, \ud558\uc9c0\ub9cc \