INSERT INTO MOVIES VALUES('MV-00693','\uc544\uc218\ub77c:\uc57c\ucfe0\uc790\uc758 \uae38','4.0','','posters/20150504_95_1430702459752gndL8_JPEG_movie_image.jpg','15\uc138 \uad00\ub78c\uac00','\ub4dc\ub77c\ub9c8','2015-05-14',75,'','1960\ub144\ub300 \ucd08\uae30, \uc77c\ubcf8\uc740 \uace0\ub3c4 \uacbd\uc81c\uc131\uc7a5\uae30\uc5d0 \ub4e4\uace0 \ud638\uacbd\uae30\ub97c \ub204\ub838\ub2e4. \ub3d9\uc2dc\uc5d0 \uc77c\ubcf8 \uc57c\ucfe0\uc790\uc758 \uaddc\ubaa8\ub294 5\ucc9c\uc5ec \uac1c \ub2e8\uccb4, 18\ub9cc \uba85\uc5d0 \uc774\ub974\ub800\uace0 10\ub144 \ub3d9\uc548 \ub9e4\ub144 8\ub9cc \uba85\uc774 \uc0c8\ub85c \ub4e4\uc5b4\uc640 \uc131\uc7a5\ud558\uc600\ub2e4. \ub2f9\uc2dc \uc57c\ucfe0\uc790\uc758 \uc591\ub300 \uc0b0\ub9e5 \uc0b0\uc559\ud68c\uc640 \ud559\uc815\ud68c\ub294 \uc11c\ub85c\ub97c \uacac\uc81c\ud558\uba70 \uc2f8\uc6c0\uc774 \ub04a\uc774\uc9c0 \uc54a\uc558\uace0 \ud56d\uc0c1 \uc77c\ucd09\uc989\ubc1c\uc758 \uacf5\uae30\uac00 \ud750\ub978\ub2e4. \ud559\uc815\ud68c\ub294 \uc0b0\uc559\ud68c\ub97c \uacbd\uacc4\ud558\uae30 \uc704\ud574 \uad00\ub3d9\uc9c0\ubc29\uc744 \uc5f0\ud569\ud588\uace0 \uadf8 \uc911\uc2ec\uc5d0\ub294 \ub2e4\ub978 \uc57c\ucfe0\uc790\ub4e4\uacfc \ub2e4\ub974\uac8c \uc758\ub9ac \uc788\uace0 \uc810\uc796\uc740 \uc218\uc644\uac00, \uc774\uc2dc\ud0c0\uac00 \uc788\uc5c8\ub2e4. \uc774\uc2dc\ud0c0\uac00 \uad00\ub3d9 \uc5f0\ud569\uc744 \uc131\uacf5\uc801\uc73c\ub85c \uc774\ub04c\uc790, \uad6d\uac00\ub294 \uc57c\ucfe0\uc790\ub97c \uacac\uc81c\ud558\uc5ec \uadf8\ub4e4\uc758 \uc8fc \uc218\uc775 \uc885\ubaa9\uc778 \ub3c4\ubc15\uacfc \uc790\ub9bf\uc138\ub97c \uc815\uc0c1\uc791\uc804\uc774\ub77c\ub294 \uc774\ub984 \ud558\uc5d0 \ubd88\uac00\ud558\uac8c \ud558\uc600\uc73c\ub098, \uc774\uc2dc\ud0c0\ub294 \uc2dc\ub300\uc5d0 \ub9de\ucdb0 \ud569\ubc95\uc801\uc73c\ub85c \uc138\uae08\uc744 \ub0b4\ub294 \uc0ac\uc5c5\uc744 \uac15\uad6c\ud558\uace0 \uac74\uc124\uc5c5\uacfc \ubd80\ub3d9\uc0b0\uc73c\ub85c \ud0c0\uce20\ubbf8 \uc0b0\uc5c5\uc744 \uc2dc\uc791\ud558\ub824 \ud