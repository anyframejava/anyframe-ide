INSERT INTO MOVIES VALUES('MV-00749','\uaf43\ubcf4\ub2e4\ucc98\ub140\uadc0\uc2e0','4.83','','posters/20141230_247_1419928013512TgcJc_JPEG_movie_image.jpg','','','2015-01-15',103,'','\uc601\uc6c5\uc740 \uc0ac\ucc44\ube5a\uc73c\ub85c \ud589\ubc29\ubd88\uba85\ub41c \uc5ec\uc790\uce5c\uad6c\ub97c \ucc3e\uae30 \uc704\ud574 \ub2e4\ub2c8\ub358 \ub300\uae30\uc5c5\uc744 \uadf8\ub9cc\ub450\uace0 \uc0ac\ucc44\ud68c\uc0ac\uc5d0 \ub4e4\uc5b4\uac00 \uadf8\ub140\uc758 \ud589\ubc29\uc744 \ucc3e\ub294\ub2e4. \uadf8\ub7ec\ub358 \uc911 \ucc44\ubb34\uc790 \ud64d\ub450\ubb38\uc744 \ub9cc\ub098\uac8c \ub41c\ub2e4. \ud64d\ub450\ubb38\uc740 \uac74\ub2ec\uc0dd\ud65c\uc744 \ud558\ub358 \uc911 \uc2e0\ub0b4\ub9bc\uc744 \ubc1b\uc544 \ubb34\ub2f9\uc774 \ub41c \uc778\ubb3c\ub85c, \uc0ac\ucc44\ub97c \uc5bb\uc5b4 \uac15\uac00\uc758 \uc624\ub798\ub41c \ud39c\uc158\uc744 \uad6c\uc785\ud55c\ub2e4. \uc218\ud589 \ub3c4\uc911 \ub9ce\uc740 \uadc0\uc2e0\ub4e4\uc774 \uc6d0\ud55c\uc744 \ud480\uc5b4 \ub2ec\ub77c\uace0 \ucc3e\uc544\uc624\uc9c0\ub9cc \uadf8\ub294 \ucc98\ub140\uadc0\uc2e0\ub4e4\ub9cc\uc744 \uace8\ub77c \uacc4\uc57d\uc744 \ub9fa\ub294\ub2e4. \ud64d\ub450\ubb38\uc758 \uc8fc \uc0ac\uc5c5 \uc544\uc774\ud15c\uc740 \uc131\uc0c1\ub2f4\uc73c\ub85c \ubd80\ubd80\ub4e4\uc758 \uc2dc\ub4e4\ud574\uc9c4 \uc131\uc0dd\ud65c\uc744 \ud68c\ubcf5\uc2dc\ud0a4\ub294 \uac83\uc774\ub2e4. \uadc0\uc2e0\ub4e4\uc740 \ubd80\uc778\uc758 \ubab8\uc5d0 \ube59\uc758\ub3fc \uc0c8\ub85c\uc6b4 \uc139\uc2a4 \ud14c\ud06c\ub2c9\uc744 \uc775\ud788\uac8c \ud558\uace0 \uadf8 \ub300\uac00\ub85c \ubd80\uc801\uc744 \ubaa8\uc544 \uc800\uc2b9\uc5d0 \uac00\uace0\uc790 \ud55c\ub2e4. \uc601\uc6c5\uc758 \ub3c4\uc6c0\uc73c\ub85c \ube5a\uc744 \uac1a\uac8c \ub41c \ud64d\ub450\ubb38\uc740 \uadc0\uc2e0\uacfc \ud568\uaed8 \uc704\ud5d8\uc5d0 \ucc98\ud55c \uc601\uc6c5\uc758 \uc5ec\uc790\uce5c\uad6c\ub97c \ub3c4\uc640\uc8fc\uae30\ub85c \ud558\ub294\ub370......','\ucd5c\uc601\ubbfc','\uc548\uc7ac\ubbfc, \ub9f9\uc2b9\uc9c0')
INSERT INTO MOVIES VALUES('MV-00750','\uc544\uba54\ub9ac\uce78 \uc139\uc2a4\ud30c\ud2f0','6.76','','posters/20150209_19_142347510380580cpp_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\ucf54\ubbf8\ub514','2014-12-30',89,'','\uace0\ub4f1\ud559\uad50 \uc5ec\ub984\ubc29\ud559\uc744 \ub9de\uc544 \uc0bc\ucd1d\uc0ac \uce5c\uad6c\ub4e4\uc778 \uc870\ucee4\uc640 \uc0e4\ud0a4\ub294 \uc22b\ucd1d\uac01\uc778 \ube44\ud2c0\uc758 \ub531\uc9c0\ub97c \ub5bc\uc5b4\uc8fc\ub824\uace0 \ub2e4 \uac19\uc774 \ud569\uc2ec\ud574 \ub3d9\ub124 \uc5ec\uc790\ub4e4\uc5d0\uac8c \ucd94\ud30c\ub97c \ub358\uc9c4\ub2e4. \uc5ec\uc790\ub97c \uaf2c\uc2dc\uae30 \uc704\ud574 \uc57c\uad6c\ubd80 \ucf54\uce58\uc5d0\uac8c \uc220 \uad6c\ub9e4\ub97c \ubd80\ud0c1\ud558\uae30\ub3c4 \ud558\uace0 \ub3c8\uc744 \ubc8c\uae30 \uc704\ud574 \ubd88\ubc95 \uc774\ubbfc\uc790\ub4e4\uacfc \ub9c9\ub178\ub3d9\uc744 \ud558\uae30\ub3c4 \ud558\uace0 \ub0a8\ud3b8 \uc788\ub294 \uc5ec\uc790\ub97c \uc720\ud639\ud588\ub2e4\uac00 \ub0a8\ud3b8\uc778 \uacbd\ucc30\uc5d0\uac8c \ucad3\uaca8 \uc904\ud589\ub791\uc744 \uce58\uae30\ub3c4 \ud55c\ub2e4. \uadf8\ub7ec\ub358 \uc911 \uc608\uc05c \uc544\uac00\uc528\ub4e4\uc774 \ud669\ud640\ud55c \ubc24\uc744 \uc120\uc0ac\ud574 \uc900\ub2e4\ub294 \u2018\ub864\ub77c\uc758 \uc0ac\ub791\ubc29\u2019 \uc18c\ubb38\uc744 \ub4e3\uac8c \ub418\uace0 \uc14b\uc740 \ub3c8\uc744 \ubaa8\uc544 \uac00\ubcf4\uae30\ub85c \ud55c\ub2e4. \uc704\uc870 \uc2e0\ubd84\uc99d\uc744 \ub9cc\ub4e4\uace0 \ub4b7\ub3c8\uc744 \uc8fc\uace0 \ub4e4\uc5b4\uac04 \uadf8 \ub9e4\ucd98\uc5c5\uc18c\uc5d0\uc11c \uc0bc\ucd1d\uc0ac\ub294 \uacfc\uc5f0 \uc22b\ucd1d\uac01 \ub531\uc9c0\ub97c \ub5c4 \uc218 \uc788\uc744\uae4c? \uc9c4\uc9dc \ub0a8\uc790\uac00 \ub418\ub294 \uae38\uc740 \uba40\uace0\ub3c4 \uac00\uae5d\ub2e4. \uccab \uc5ec\uc790 \uacbd\ud5d8\uc744 \uc704\ud574 \uc21c\uc9c4\ud55c \uc138 \uba85\uc758 \uc22b\ucd1d\uac01\ub4e4\uc774 \ubc8c\uc774\ub294 \uc88c\ucda9\uc6b0\ub3cc \uc131\uc7a5\uae30\uac00 \uc2dc\uc791\ub418\ub294\ub370...','\ud328\ud2b8\ub9ad C. \ud398\ub808\uc988','\uc544\ub2e8 \ub9c8\ud2f4 \ub85c\uce74, \ub85c\ub2c8 \uc54c\ubc14\ub808\uc988')
INSERT INTO MOVIES VALUES('MV-00751','\ub77c\uc2a4\ud2b8\uac74\ub9e8','3.0','','posters/20141218_229_141888731051441SPx_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\uc561\uc158','2014-12-23',84,'','\uc544\uc77c\ub79c\ub4dc \ubca8\ud30c\uc2a4\ud2b8. IRA\ub294 \uc790\uc720\ub97c \uc218\ud638\ud558\uae30 \uc704\ud55c \uad70 \uc870\uc9c1\uc774\uc5c8\uc9c0\ub9cc \ub9c8\uc57d\ud310\ub9e4\uc640 \ud14c\ub7ec\ub9ac\uc2a4\ud2b8 \ud6c8\ub828\uc744 \uc77c\uc0bc\ub294 \uad6d\uc81c \ubc94\uc8c4\uc870\uc9c1\uc73c\ub85c \ubcc0\uc9c8\ub418\uc5c8\ub2e4. \ubbf8\ud0a4\ub294 \ub9c8\uc57d\uac70\ub798\ub97c \ud558\ub358 \uc911, \uc774\uc720\ub3c4 \uc5c6\uc774 \uc7a5\uc560\uc790\ub97c \uc8fd\uc774\ub824\ub358 \ub3d9\ub8cc \uba38\uc11c\ub97c \uc8fd\uc774\uac8c \ub41c\ub2e4. \uc870\uc9c1\uc5d0\uac8c \ucad3\uae30\uac8c \ub41c \ubbf8\ud0a4\ub294 LA\ub85c \uac00\uc11c \uc0c8\ub85c\uc6b4 \uc0b6\uc744 \uc2dc\uc791\ud558\ub824 \ud558\uc9c0\ub9cc \uc5ec\uc790 \uce5c\uad6c \ub2c8\ub098\uc758 \uc624\ube60 \uc9c0\ubbf8\ub85c \uc778\ud574 \ub610\ub2e4\uc2dc \ub9c8\uc57d\uac70\ub798\uc5d0 \uc5f0\ub8e8\ub418\uc5b4 \uc5b4\uca54 \uc218 \uc5c6\uc774 \uc9c0\ubbf8\ub97c \ub3c4\uc640\uc8fc\uc5b4\uc57c \ud558\ub294 \uc0c1\ud669\uc5d0 \ubab0\ub9b0\ub2e4. \ud558\uc9c0\ub9cc \uc77c\uc774 \ud574\uacb0\ub418\uae30\ub294\ucee4\ub155 \uc810\uc810 \ub354 \uaf2c\uc5ec\uac00\uae30\ub9cc \ud558\ub354\ub2c8 \ubb34\uc790\ube44\ud55c \ub9c8\uc57d\uac70\ub798 \uc870\uc9c1\uc740 \uc9c0\ubbf8\uc640 \ub9c8\uc57d\ub2e8\uc18d\ubc18 \uc704\uc7a5\uc694\uc6d0\uae4c\uc9c0 \uc8fd\uc774\uac8c \ub418\uace0 \ubbf8\ud0a4\ub9cc \uaca8\uc6b0 \ube60\uc838 \ub098\uc628\ub2e4. \ud558\uc9c0\ub9cc \ud5d8\ub09c\ud55c \ubbf8\ud0a4\uc758 \uc778\uc0dd\uc740 \uc5ec\uae30\uc11c \uadf8\uce58\uc9c0 \uc54a\ub294\ub2e4. \uc8fd\uc600\ub2e4\uace0 \ubbff\uc5c8\ub358 \uba38\uc11c\uac00 LA\uae4c\uc9c0 \uc640\uc11c \ub2c8\ub098\ub97c \ub0a9\uce58\ud558\uace0 \ubbf8\ud0a4\ub97c \ubd80\ub974\ub294\ub370...','\ub098\ub2e8 \ud504\ub7ad\ucf54\uc2a4\ud0a4','\ub9ad \ud06c\ub85c\ud3ec\ub4dc, \ub9c8\ud2f0 \ub9e5\uacfc\uc774\uc5b4')
INSERT INTO MOVIES VALUES('MV-00752','\ubc8c\uc9d1\uc758 \uc544\uc774\ub4e4','8.0','','posters/20141208_70_1418027784172HJCFu_JPEG_movie_image.jpg','','','2014-12-10',86,'','\uc804\uad6d \uac01\uc9c0\uc5d0\uc11c \ubaa8\uc5ec\ub4e0 \uc804\uc7c1\uace0\uc544 \uc5f4 \uba85\uc774 \ubb34\ub9ac\ub97c \uc9c0\uc5b4 \uc0dd\ud65c\ud55c\ub2e4. \uc624\uac08 \ub370 \uc5c6\ub358 \uc544\uc774\ub4e4\uc740 \uacbd\ucc30\uc5d0 \ubd99\uc7a1\ud788\uc9c0 \uc54a\uc73c\ub824 \ub298 \uae34\uc7a5\ud558\uba74\uc11c\ub3c4, \uae30\ucc28\uc5ed \uadfc\ucc98\ub97c \ubc30\ud68c\ud558\uba70 \uc4f0\ub808\uae30\ud1b5\uc744 \ub4a4\uc9c0\uac70\ub098 \uc678\ub2e4\ub9ac \ubd80\ub791\uc790\ub97c \ub530\ub77c\ub2e4\ub2c8\uba70 \uc554\uc2dc\uc7a5 \uc77c\uc744 \ud558\uae30\ub3c4 \ud55c\ub2e4. \uadf8\ub7f0\ub370 \uc544\uc774\ub4e4 \uc55e\uc5d0 \ud55c \uad70\uc778\uc774 \ub098\ud0c0\ub09c\ub2e4. \uc804\uc7c1 \ud6c4 \ubcf8\uad6d\uc5d0 \ub3cc\uc544\uc628 \uadf8\ub294 \uc544\uc774\ub4e4\uacfc \ub9c8\ucc2c\uac00\uc9c0\ub85c \uace0\uc544\uc774\uba70, \uc5ed\uc2dc \ub3cc\uc544\uac08 \uacf3\uc774 \uc5c6\ub2e4. \uadf8\ub294 \uc544\uc774\ub4e4\uc744 \uc774\ub04c\uace0 \uc790\uc2e0\uc774 \uc790\ub790\ub358 \uace0\uc544\uc6d0\uc73c\ub85c \uac00\uae30\ub85c \uacb0\uc2ec\ud55c\ub2e4. \uc2dc\ubbf8\uc988\uac00 \uc1fc\uce58\ucfe0\ub97c \ub5a0\ub098 \ub9cc\ub4e0 \ub3c5\ub9bd\uc601\ud654\uc0ac\uc5d0\uc11c \ucc98\uc74c \ub9cc\ub4e0 \uc791\ud488\uc73c\ub85c, \uadf8\ub294 \uba87 \ub144 \ud6c4 \uc18d\ud3b8\uc744 \ub9cc\ub4e4\uae30\ub3c4 \ud588\ub2e4. \uc2dc\ubbf8\uc988\ub294 \uc544\uc774\ub4e4\uc758 \uc5ec\uc815\uc744 \ub530\ub77c\uac00\uba70 \uc804\ud6c4 \uc77c\ubcf8\uc758 \ud669\ud3d0\ud55c \uae38\uacfc \ucca0\ub3c4, \uc0b0\uacfc \ud56d\uad6c \ub4f1\uc744 \ub2f4\ub294\ub2e4. \ud604\uc9c0 \ucd2c\uc601\uc744 \ud1b5\ud574 \uc0ac\uc2e4\uc801\uc778 \ubaa8\uc2b5\uc744 \ub2f4\uc544\ub0b8 \uc2dc\ubbf8\uc988\ub294 \ud788\ub85c\uc2dc\ub9c8\uc758 \ud3d0\ud5c8\ub97c \ub0b4\ub824\ub2e4\ubcf4\ub294 \uadf9\uc801\uc778 \uc21c\uac04\uc744 \ub9cc\ub4e4\uc5b4\ub0b4\uae30\ub3c4 \ud588\ub2e4.','\uc2dc\ubbf8\uc988 \ud788\ub85c\uc2dc','\ub9c8\uc0ac\ucf54 \ub098\uce20\ud0a4, \uace0\uc1fc \uc1fc\uc774\uce58')
INSERT INTO MOVIES VALUES('MV-00753','\uac00\ub140\uc758 \uac01\uc11c','.0','','posters/20141208_290_1418028620477n2dw4_JPEG_movie_image.jpg','','','2014-12-10',98,'','\uba54\uc774\uc9c0 34\ub144, \uc720\ub791\uadf9\ub2e8\uc774 \uc2dc\uace8 \uc5ec\uad00\uc5d0 \ubb35\ub294\ub2e4. \uadf9\ub2e8\uc758 \ub0a8\uc790\ub4e4\uc740 \ucc28 \ub3c4\ub9e4\uc0c1\uc5d0\uac8c \uc720\uc77c\ud55c \uc5ec\ubc30\uc6b0 \uc6b0\ud0c0\uc758 \ucda4\uc744 \ubcf4\uc5ec\uc8fc\uace0\ub294 \uadf8\ub140\ub97c \ud314\uc544\ubc84\ub9ac\ub824 \ud55c\ub2e4. \uc6b0\ud0c0\ub97c \ub531\ud558\uac8c \uc5ec\uae34 \ub3c4\ub9e4\uc0c1\uc740 \uadf8\ub140\ub97c \ub370\ub9ac\uace0 \uadc0\uac00\ud558\ub294\ub370, \uac00\uc871\uc740 \ubb3c\ub860 \ud558\uc778\ub4e4\ub9c8\uc800\ub3c4 \uc6b0\ud0c0\ub97c \ubc14\ub77c\ubcf4\ub294 \uc2dc\uc120\uc774 \uacf1\uc9c0 \uc54a\ub2e4. \uadf8\ub7f0\ub370 \uc8fc\uc778\uc774 \uac11\uc790\uae30 \uc138\uc0c1\uc744 \ub728\uace0, \ubaa8\ub450\ub4e4 \ud63c\ub780\uc5d0 \ube60\uc9c4\ub2e4. \ub3c4\ucfc4\uc5d0\uc11c \ub300\ud559\uc744 \ub2e4\ub2c8\ub358 \uc7a5\ub0a8 \uc1fc\ud0c0\ub85c\uac00 \ub3cc\uc544\uc624\uc9c0\ub9cc \uac00\uac8c\ub97c \ub2e4\uc2dc \uc77c\uc73c\ud0a4\uae30\ub780 \uc27d\uc9c0 \uc54a\ub2e4. \ub300\ud559\uc744 \uadf8\ub9cc\ub450\ub824\ub294 \uc1fc\ud0c0\ub85c\uc5d0\uac8c \uc6b0\ud0c0\ub294 \uc790\uc2e0\uc774 \uac00\uac8c\ub97c \ub9e1\uc744 \ud14c\ub2c8 \ud559\uc5c5\uc744 \ub9c8\uce58\ub77c \ud55c\ub2e4. \uc2dc\ubbf8\uc988\ub294 \uc2e0\ud30c\uc801 \uc18c\uc7ac\ub97c \ub2e4\ub8e8\uba74\uc11c\ub3c4 \ud2b9\uc720\uc758 \uc2a4\ud0c0\uc77c\uc744 \uc783\uc9c0 \uc54a\ub294\ub2e4. \uc720\ub791\uadf9\ub2e8\uc774 \uc0b0\uae38\uc744 \uac77\ub294 \uc7a5\uba74, \uc57c\uc678\uc640 \uc2e4\ub0b4\ub97c \ud55c \ubc88\uc5d0 \ub2f4\uc544\ub0b8 \uc7a5\uba74 \ub4f1\uc5d0\uc11c \uadf8\uac00 \uce74\uba54\ub77c\ub97c \ub2e4\ub8e8\ub294 \ub2a5\uc218\ub2a5\ub780\ud55c \uc19c\uc528\ub97c \ub9db\ubcfc \uc218 \uc788\ub2e4. \uc8fc\uc5f0\uc744 \ub9e1\uc544 \uc5f4\uc5f0\ud55c \uc2e0\ud30c\uadf9\uc758 \ub300\u