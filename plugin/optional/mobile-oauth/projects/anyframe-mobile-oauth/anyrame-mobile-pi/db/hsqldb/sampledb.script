INSERT INTO MOVIES VALUES('MV-00704','\ube4c\ub9ac\uc640 \uc6a9\uac10\ud55c \ub140\uc11d\ub4e4 3','7.36','8.17','posters/20150406_288_1428283768751e6YQc_JPEG_movie_image.jpg','\uc804\uccb4 \uad00\ub78c\uac00','\uc560\ub2c8\uba54\uc774\uc158','2015-04-30',80,'','\uc815\uae00 \uc0dd\ud65c\uc774 \ud798\ub4e4\uae30\ub9cc \ud55c \uc678\ud1a8\uc774 \uac1c\uad6c\ub9ac \u2018\ube4c\ub9ac\u2019\ub294 \uc790\uc2e0\uc774 \uc6d0\ub798 \uc0ac\ub78c\uc774\uc5c8\ub2e4\ub294 \ub9d0\ub9cc \ubbff\uace0, \ub2e4\uc2dc \uc0ac\ub78c\uc774 \ub418\uae30 \uc704\ud574 \uc778\uac04\ub4e4\uc758 \ub3c4\uc2dc\ub85c \uac00\uaca0\ub2e4\uace0 \uacb0\uc2ec\ud55c\ub2e4. \uadf8\uc758 \uafc8\uc744 \uc751\uc6d0\ud558\ub294 \uc808\uce5c \ub0a0\ub2e4\ub78c\uc950 \u2018\uc0cc\ub514\u2019, \uc218\ub2e4\uc7c1\uc774 \uc575\ubb34\uc0c8 \u2018\ud14c\ub7f0\uc2a4\u2019, \uadf8\ub9ac\uace0 \uce5c\uc808\ud55c \ud558\ub9c8 \ubd80\ubd80 \u2018\uc62c\ub9ac\u2019\uc640 \u2018\uc870\uc870\u2019\ub3c4 \u2018\ube4c\ub9ac\u2019\uc758 \uc5ec\ud589\uc5d0 \ud568\uaed8 \ud55c\ub2e4. \ud558\uc9c0\ub9cc \uc0ac\ub098\uc6b4 \uc545\uc5b4\uc758 \uacf5\uaca9\uacfc \ubb34\uc11c\uc6b4 \uc778\uac04\ub4e4\uae4c\uc9c0 \ub9cc\ub098\uba74\uc11c \uc544\uc2ac\uc544\uc2ac\ud55c \ubaa8\ud5d8\uc744 \ud558\uac8c \ub418\ub294 \u2018\ube4c\ub9ac\u2019\uc640 \uc6a9\uac10\ud55c \ub140\uc11d\ub4e4! \uacfc\uc5f0 \u2018\ube4c\ub9ac\u2019\uc640 \uc6a9\uac10\ud55c \ub140\uc11d\ub4e4\uc758 \uc815\uae00 \uc5b4\ub4dc\ubca4\ucc98\ub294 \uc131\uacf5\ud560 \uc218 \uc788\uc744\uae4c?','\ucc99 \ud30c\uc6cc\uc2a4','\uc5c4\uc0c1\ud604, \uc870\ud604\uc815')
INSERT INTO MOVIES VALUES('MV-00705','\ub9ac\ubca4\uc9c0\ud3ec\ub974\ub178 \uc5ec\ubc30\uc6b0\uc9c0\ub9dd\uc0dd','.0','','posters/m_movie_im_blank.gif','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\uba5c\ub85c/\ub85c\ub9e8\uc2a4','2015-03-19',80,'','\uc5ec\ubc30\uc6b0 \uc9c0\ub9dd\uc0dd\uc778 \ub9c8\ub9ac\ub294 \uc2ec\uc2ec\ud480\uc774\ub85c \ud574\ubcf8 SNS\uc5d0\uc11c \u2018\ucd78\u2019\uc640 \ub9cc\ub098 \uc0ac\ub791\uc744 \ud558\uac8c \ub41c\ub2e4. \uc2dc\uac04\uc774 \uac08\uc218\ub85d \uadf8\ub294 \ub098\uccb4\uc0ac\uc9c4\uacfc \uc601\uc0c1\uc744 \uc694\uad6c\ud558\uace0, \ub9c8\ub9ac\ub294 \ub9c8\uc9c0\ubabb\ud574 \uc751\ud558\uace0 \uc788\uc5c8\ub294\ub370.. \uacfc\ubb35\ud558\uace0 \ucc28\uac11\uc9c0\ub9cc \uc5f0\uae30\uc9c0\ub3c4\uc5d0 \uc788\uc5b4\uc11c \ub9cc\ud07c\uc740 \ucd5c\uace0\uc778 \uc5f0\ucd9c\uac00 \ud788\ud1a0\uc2dc. \uc804\ub3c4\uc720\ub9dd\ud55c \uc81c\uc790\uc758 \uc0ac\uc0dd\ud65c\uc774 \uc720\ucd9c\ub418\uc5c8\ub2e4. \uc81c\uc790\ub97c \uc774\ub300\ub85c \uc88c\uc808\ud558\uac8c \ub458 \uc218\ub294 \uc5c6\ub294 \uadf8\ub294 \u2018\ucd78\u2019\ub97c \ucc3e\uc544\uac00 \uc751\uc9d5\uc744 \uac00\ud55c\ub2e4.','\ud558\ub274 \ucf04\uc9c0','\ub098\ub098\ubbf8 \ub098\ub098, \ubaa8\ud1a0\uc57c\ub9c8 \uc694\uc2dc\ub178')
