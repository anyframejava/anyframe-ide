INSERT INTO MOVIES VALUES('MV-00514','\uce7c\ub9b4 \uc9c0\ube0c\ub780\uc758 \uc608\uc5b8\uc790','8.49','8.82','posters/20150821_181_14401213912286phjP_PNG_movie_image.jpg','\uc804\uccb4 \uad00\ub78c\uac00','\uc560\ub2c8\uba54\uc774\uc158','2015-09-10',85,'','\uc544\ube60\ub97c \uc783\uace0 \ub9c8\uc74c\uc744 \ub2eb\uc740 \ub538 \uc54c\ubbf8\ud2b8\ub77c \ub54c\ubb38\uc5d0 \uac71\uc815\uc774 \ub9ce\uc740 \uce74\ubc00\ub77c\ub294 \uc0ac\ub78c\ub4e4\uc744 \uc120\ub3d9\ud55c\ub2e4\ub294 \uc774\uc720\ub85c \uac10\uae08\ub41c \uc2dc\uc778 \ubb34\uc2a4\ud0c0\ud30c\uc758 \uc9d1\uc5d0\uc11c \uc77c\ud55c\ub2e4. \uadf8\ub140\ub294 \uc54c\ubbf8\ud2b8\ub77c\ub97c \ud63c\uc790 \ub458 \uc218 \uc5c6\uc5b4 \uc790\uc2e0\uc758 \uc77c\ud130\ub85c \ub370\ub824\uac00\uace0 \ubb34\uc2a4\ud0c0\ud30c\ub294 \uc54c\ubbf8\ud2b8\ub77c\uc758 \uc18c\uc911\ud55c \uce5c\uad6c\uac00 \ub41c\ub2e4. \ub9c8\uce68\ub0b4 \ubb34\uc2a4\ud0c0\ud30c\uac00 \ud480\ub824\ub098 \uace0\ud5a5\uc73c\ub85c \ub5a0\ub098\ub294 \ub0a0, \uadf8\uc758 \uc5ec\uc815\uc5d0 \ubab0\ub798 \ub3d9\ud589\ud574 \uadf8\uac00 \ub9c8\uc744 \uc0ac\ub78c\ub4e4\uc5d0\uac8c \ub098\ub220\uc8fc\ub294 \uc0b6\uc758 \uc9c4\ub9ac\uc640 \uc544\ub984\ub2e4\uc6b4 \uc774\uc57c\uae30\ub97c \ub4e4\uc73c\uba70 \uc870\uae08\uc529 \ub9c8\uc74c\uc744 \uc5f4\uc5b4\uac00\ub294 \uc54c\ubbf8\ud2b8\ub77c. \uadf8\ub7ec\ub358 \uc911, \uc54c\ubbf8\ud2b8\ub77c\ub294 \ubb34\uc2a4\ud0c0\ud30c\uac00 \uace0\ud5a5\uc73c\ub85c \ud480\ub824\ub098\ub294 \uac83\uc774 \uc544\ub2cc \uadf8\ub97c \ub458\ub7ec\uc2fc \ub2e4\ub978 \uc74c\ubaa8\uac00 \uc788\uc74c\uc744 \uc54c\uac8c \ub418\ub294\ub370... \uc0ac\ub791, \uacb0\ud63c, \uc77c, \uc544\uc774\ub4e4, \uc74c\uc2dd, \uc120\uacfc \uc545, \uc8fd\uc74c, \uadf8\ub9ac\uace0 \uc790\uc720 \uc6b0\ub9ac \ubaa8\ub450\uac00 \uacf5\uac10\ud560 \uc0b6\uc758 \uc9c4\ub9ac\uac00 \uc544\ub984\ub2e4\uc6b4 \uc601\uc0c1\uacfc \uc74c\uc545\uc73c\ub85c \ud669\ud640\ud558\uac8c \ud3bc\uccd0\uc9c4\ub2e4!','\ub85c\uc800 \uc54c\ub7ec\uc2a4','\ub9ac\uc