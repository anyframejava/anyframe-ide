INSERT INTO MOVIES VALUES('MV-00089','\uc774\ubd88','5.97','','posters/20141205_282_14177573747382GlBM_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\ucf54\ubbf8\ub514','2014-12-30',103,'','\uacbd\uae30 \ubd88\ud669\uc5d0 \ub530\ub978 \ud68c\uc0ac\uc2e4\uc801\uc545\ud654\uc640 \ud53c\ube44\ub9b0\ub0b4 \ub098\ub294 \uace0\uacfc\ud3c9\uac00\uae30\uac04\uc744 \ucf54\uc55e\uc5d0 \ub450\uace0 \ud559\uad50 \uc120\ubc30\uc778\uc0c1\ubb34\uc758 \uacc4\ub7b5\uc73c\ub85c \ubcc0\ud0dc\uc12d \ubd80\uc7a5\uc774 \ubb34\ub9ac\ud55c \ud300 \uc57c\uc720\ud68c\ub97c \ucd94\uc9c4\ud55c\ub2e4. \uadf8\ub7ec\ub098 \uace0\ubbfc\uc774 \ub108\ubb34 \uc55e\uc11c \uc220\uc5d0 \ucde8\ud55c \ubcc0\ud0dc\uc12d \ubd80\uc7a5\uc758 \ub9d0 \uc2e4\uc218\uc5d0 \ubaa8\ub4e0 \uc2e4\uc801\uc758 \ucc45\uc784\uc744 \uc2a4\uc2a4\ub85c \ucc45\uc784\uc9c0\ub294 \uaf34\uc774 \ub418\uc5b4 \ubc84\ub9b0\ub2e4. \uace0\uacfc\uc758 \uc555\ubc15\uc5d0\uc11c \ubc97\uc5b4\ub09c \uc9c1\uc6d0\ub4e4\uc774 \uc220\uc5d0 \ucde8\ud574 \ubed7\uc5b4\ubc84\ub9b0 \ubd80\uc7a5\uc744 \ub0b4\ubc84\ub824 \ub450\uace0 \uc990\uac70\uc6b4 \uc9c4\uc2e4\uac8c\uc784\uc744 \uc2dc\uc791\ud55c\ub2e4. \ud3c9\uc18c\uc5d0 \uc774\uc9c4\uacbd \uacfc\uc7a5\uc774 \uac15\ub9cc\ud638 \ub300\ub9ac\ub97c \uc560\ud2cb\ud558\uac8c \ucc59\uaca8\uc8fc\ub294 \uc774\uc720\uc640 \ubd80\uc7a5\uc758 \uc57d\uc810\uc744 \uc7a1\uc740 \uc720\ub77c\uc758 \ud669\ub2f9\ud558\uace0 \ub2f9\ub2f9\ud55c \ud589\ub3d9\ub4e4\uc758 \uc774\uc720\uac00 \uc9c4\uc2e4\uac8c\uc784\uc744 \ud1b5\ud574 \ub9c8\uce58 \ub2e4\ub978 \uc0ac\ub78c\uc758 \uc774\uc57c\uae30 \uc778 \ub0e5 \ud480\uc5b4\uac00\uba70 \uc774\ubd88\uacfc \uad00\ub828\ub41c \uadf8 \uc9c4\uc2e4\uc774 \ud558\ub098 \ub458\uc529 \ubc1d\ud600\uc9c0\ub294\ub370\u2026.','\uae40\ud55c\uc900','\uc815\uc774\uc2ac, \uc131\uc778\uc11d')
INSERT INTO MOVIES VALUES('MV-00090','\ub2f9\uc2e0\uc758 \ubcbd\uc774 \uc18d\uc0ad\uc77c\ub54c','9.0','7.17','posters/20141210_171_1418184959529otp5L_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\uc2a4\ub9b4\ub7ec','2014-12-18',95,'','\ubca0\ub97c\ub9b0\uc5d0 \uc788\ub294 \ub300\ud559\uc5d0 \uc9c4\ud559\ud55c \ub9c8\ud2f4\uc740 \uc6b4 \uc88b\uac8c \uc800\ub834\ud558\uace0 \ub0a1\uc740 \uc544\ud30c\ud2b8\ub97c \uacc4\uc57d\ud55c\ub2e4. \uadf8\ub294 \uc587\uc740 \ubcbd\uc744 \uc0ac\uc774\uc5d0 \ub450\uace0 \uc774\uc6c3\ud574\uc788\ub294 \uc9d1\uc8fc\uc778\uc778 \ub9e4\ud639\uc801\uc778 \uc2dc\ubaac\uc744 \ucc98\uc74c \ubcf8 \uc21c\uac04\ubd80\ud130 \ub2e8\uc228\uc5d0 \ub9c8\uc74c\uc744 \ube7c\uc557\uae30\uace0 \uc721\uccb4\uc801 \uad00\uacc4\ub97c \ud0d0\ub2c9\ud558\uac8c \ub41c\ub2e4. \uc804 \uc138\uc785\uc790\uac00 \ud754\uc801\ub3c4 \uc5c6\uc774 \uc0ac\ub77c\uc84c\ub2e4\ub294 \uc0ac\uc2e4\uc774 \uc11d\uc5f0\uce58 \uc54a\uc558\uc73c\ub098, \uc2e0\ube44\ud55c \ub9e4\ub825\uc758 \uc2dc\ubaac\uc5d0\uac8c \uac77\uc7a1\uc744 \uc218 \uc5c6\uc774 \ube60\uc838\ub4e4\uc5b4 \uadf8\ub140 \uc8fc\ubcc0\uc5d0\uc11c \uc77c\uc5b4\ub098\ub294 \ubbf8\uc2a4\ud130\ub9ac\ud55c \uc77c\ub4e4\uc744 \ub9dd\uac01\ud558\uac8c \ub418\ub294\ub370\u2026','\uadf8\uc81c\uace0\uc2dc \ubb34\uc2a4\uce7c\ub77c','\ube48\uc13c\ud2b8 \ub808\ub370\uce20\ud0a4, \uce74\ud0c0\ub9ac\ub098 \ud5e4\uc774\uc5b4')
INSERT INTO MOVIES VALUES('MV-00091','71: \ubca8\ud30c\uc2a4\ud2b8\uc758 \ub208\ubb3c','9.0','7.88','posters/20160713_136_1468372746974HSY5f_JPEG_movie_image.jpg','15\uc138 \uad00\ub78c\uac00','\uc561\uc158','2016-07-21',99,'','1971\ub144 \ubd81\uc544\uc77c\ub79c\ub4dc, \uc5b4\ub9b0 \ub3d9\uc0dd\uc758 \ubd80\uc591\uc744 \uc704\ud574 \uad70\ub300\uc5d0 \uc9c0\uc6d0\ud558\uac8c \ub41c \uac8c\ub9ac \ud6c4\ud06c. \uadf8\ub294 \uad6d\ub0b4 \ubd84\uc7c1 \uc9c0\uc5ed\uc758 \uc2dc\uc704\ub300 \uc9c4\uc555\uc744 \uc704\ud574 \ubca8\ud30c\uc2a4\ud2b8\ub85c \ud30c\ubcd1\ub41c\ub2e4. \uc815\ubd80 \uacbd\ucc30\uc758 \ud3ed\ub825\uc801\uc778 \uc9c4\uc555\uacfc \uacfc\uaca9\ud55c \uad6c\uad50\ub3c4\uc758 \ub370\ubaa8\uc758 \ud63c\ub780 \uc18d, \uac8c\ub9ac \ud6c4\ud06c\ub294 \ub3d9\ub8cc\uc640 \uc801\ub300\uc801\uc778 \uc138\ub825\uc774 \uac00\ub4dd\ud55c \uc9c0\uc5ed \uc18d\uc5d0 \uace0\ub9bd\ub418\uac8c \ub418\uace0, \uae09\uae30\uc57c \ud568\uaed8 \ub099\uc624\ub41c \ub3d9\ub8cc\ub294 \ubd81\uc544\uc77c\ub79c\ub4dc \ub3c5\ub9bd\uc744 \ucd94\uc9c4\ud558\ub294 \uae09\uc9c4 \ubb34\uc7a5\ud30c\uc5d0 \uc8fd\uc784\uc744 \ub2f9\ud55c\ub2e4. \ucd1d\uc54c\uc774 \ube57\ubc1c\uce58\ub294 \ubca8\ud30c\uc2a4\ud2b8, \uadf8\ub294 \ubb34\uc0ac\ud788 \uc0b4\uc544\ub0a8\uc544 \ub3cc\uc544\uac08 \uc218 \uc788\uc744 \uac83\uc778\uac00 \u2026.','\uc580 \ub514\ub9e8\uc9c0','\uc7ad \uc624\ucf54\ub12c, \uc200 \ud574\ub9ac\uc2a4')
