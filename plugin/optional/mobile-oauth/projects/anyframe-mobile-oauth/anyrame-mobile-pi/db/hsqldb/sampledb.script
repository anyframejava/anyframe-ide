INSERT INTO MOVIES VALUES('MV-00682','\uc544\uc90c\ub9c8...\uc90c\ub9c8\ub4e4\uc758\ubc18\ub780','9.29','','posters/20150622_231_1434934488222mk8xj_JPEG_movie_image.jpg','\uccad\uc18c\ub144 \uad00\ub78c\ubd88\uac00','\ub4dc\ub77c\ub9c8','2016-01-07',99,'','\uc11c\uc6b8 \uc678\uacfd\uc758 \uc5b4\ub290 \ub9c8\uc744\uc758 \ubd80\ub140\ud68c\uc7a5 \u2018\ubbf8\uc601\u2019\uc740 \ub3d9\ub124 \uacf3\uacf3\uc5d0\uc11c \ubc8c\uc5b4\uc9c0\ub294 \uc131\ud3ed\ub825, \uac00\uc815\ud3ed\ub825, \ud559\uad50 \ud3ed\ub825, \ubd88\ub7c9\uc2dd\ud488 \ud1f4\uce58\uc5d0 \uc628 \ud798\uc744 \uc3df\ub294 \ubbf8\ubaa8\uc640\ub294 \ub2ec\ub9ac \uc5b5\ucc99\uc2a4\ub7ec\uc6b4 \ub610\uc21c\uc774 \uac19\uc740 \uc544\uc90c\ub9c8\uc774\ub2e4. \uadf8\ub7f0 \u2018\ubbf8\uc601\u2019\uc5d0\uac8c\ub294 \ucd08\ub4f1\ud559\uc0dd \ub538 \u2018\uc9c0\uc740\u2019\uacfc \ubc14\ub78c\ub465\uc774 \ub0a8\ud3b8\uc774 \uc788\ub294\ub370 \ub0a8\ud3b8 \u2018\uc778\ucca0\u2019\uc740 \ubd80\ub140\ud68c\uc7a5\uc778 \uc544\ub0b4\uc640\ub294 \ub2ec\ub9ac \ub3d9\ub124\uc5d0\uc11c \ube44\ubc00 \ub304\uc2a4\uad50\uc2b5\uc18c\ub97c \uc6b4\uc601\ud558\uba70 \ub3c8 \ub9ce\uc740 \uacfc\ubd80\ub098 \uc720\ubd80\ub140\ub97c \uc720\ud639\ud558\uc5ec \ubd88\ub95c\uc744 \uc800\uc9c0\ub974\uba70 \ud55c\ud0d5\uc744 \uafc8\uafb8\ub294 \ubc31\uc218\uac74\ub2ec\uc774\ub2e4. \u2018\ubbf8\uc601\u2019\uc740 \ub9e4\uc77c \uc544\uce68 \ucd08\ub4f1\ud559\uc0dd \ub538\uc744 \ubd88\ub7c9\ubc30\ub4e4\ub85c\ubd80\ud130 \ubcf4\ud638\ud558\uae30 \uc704\ud574 \ub4f1\uad50\ub97c \ub3c4\uc640\uc8fc\uba70 \ub3d9\ub124 \uace8\ubaa9 \uc548\uc5d0\uc11c \ubc1c\uc0dd\ud558\ub294 \uac01\uc885 \ud559\uad50\ud3ed\ub825, \uc131\ucd94\ud589 \ub4f1\uc758 \ud604\uc7a5\uc744 \ubaa9\uaca9\ud558\uba74 \ud638\ub8e8\ub77c\uae30\ub97c \ubd88\uc5b4 \uadf8\ub4e4\uc744 \uacbd\ucc30\uc5d0 \ub118\uae30\ub294 \ub4f1, \ud65c\ub2ec\ud55c \ud589\ub3d9\ubc18\uacbd\uc73c\ub85c \ub9c8\uc744 \uc815\ud654 \uc6b4\ub3d9\uc5d0 \uc55e\uc7a5\uc744 \uc120\ub2e4. \uadf8 \ub3d9\ub124\uc5d0\ub294 \uc545\uba85 \ub192\uc740 \uc0ac\ucc44\uc5c5\uc790 \u2018\ubc15\uac00\u2019\uc640 \ubd88\ub7c9\uc2dd\ud488\uc744 \uc0ac\uc6a9\ud558\uba70 \uc5ec\uc885\uc5c5\uc6d0\uc744 \uc0c1\uc2b5\uc801\uc73c\ub85c \uc131 \ucd94\ud589\ud558\ub294 \uc911\uad6d\uc9d1 \u2018\uc655 \uc0ac\uc7a5\u2019\uacfc \uc74c\ub780\ubb3c\uc744 \ub9cc\ub4dc\ub294 \ubd88\ub7c9\uc5c5\uc790\uc778 \u2018\uc591\uac10\ub3c5\u2019 \ub4f1 \uc545\uc758 \ucd95 3\uc778\ubc29\uc774 \ub3d9\ub124\ub97c \ubb38\ub780\ucf00 \ud558\ub294 \ube44\ub9ac\ub97c \uc800\uc9c0\ub974\uace0 \uc788\ub2e4. \ud638\ub8e8\ub77c\uae30 \ubd80\ub140\ud68c\uc7a5 \u2018\ubbf8\uc601\u2019\uc740 \ubd80\ub140\ud68c\uc6d0\ub4e4\uacfc \ud568\uaed8 \uadf8\ub7f0 \uc545\uc758 \ubb34\ub9ac\ub4e4\uacfc \ub9e4\uc77c\uac19\uc774 \uce58\uc5f4\ud55c \uc804\uc7c1\uc744 \ud55c\ub2e4. \ub54c\ub85c\ub294 \u2018\ubbf8\uc601\u2019\uc774 \ubd88\ub7c9\ud559\uc0dd\ub4e4\uc5d0\uac8c \uc131\ud3ed\ud589\uc744 \ub2f9\ud558\ub824\ub294 \uc21c\uac04, \uc704\uae30\uc5d0\uc11c \ud0c8\ucd9c\uc2dc\ud0a4\uae30\ub3c4 \ud558\uace0, \ubd88\ub95c\ud604\uc7a5\uc744 \ud3ed\ub85c\ud558\uac70\ub098 \ub0a8\ud3b8 \u2018\uc778\ucca0\u2019\uc758 \ub304\uc2a4\uad50\uc2b5\uc18c\ub97c \ud3d0\uc1c4\uc2dc\ud0a4\ub294 \ub4f1 \ubcf8\uaca9\uc801\uc778 \uc0ac\ud68c\uc545\uc73c\ub85c\ubd80\ud130\uc758 \uc804\uc7c1\uc744 \ubc8c\uc778\ub2e4. \uadf8\ub7ec\ub358 \uc5b4\ub290 \ub0a0 \ubbf8\uc601\uc758 \ucd08\ub4f1\ud559\uc0dd \ub538 \u2018\uc9c0\uc740\u2019\uc774 \u2018\ubbf8\uc601\u2019\uc5d0\uac8c \uc6d0\ud55c\uc744 \ud488\uc740 \uc545\ub355 \uc0ac\ucc44\uc5c5\uc790 \u2018\ubc15\uac00\u2019\uc5d0\uac8c \uc131\ucd94\ud589\uc744 \ub2f9\ud558\ub294\ub370...','\uae40\ubb38\uc625','\uae40\ubbf8\uc601, \uc8fc\ud558\ub9b0')
INSERT INTO MOVIES VALUES('MV-00683','\uc138\ube44\uc57c\uc758 \uc774\ubc1c\uc0ac','9.5','8.73','posters/20150527_85_1432710652253XOEuY_JPEG_movie_image.jpg','\uc804\uccb4 \uad00\ub78c\uac00','\uacf5\uc5f0\uc2e4\ud669','2015-06-06',205,'','\ub85c\uc9c0\ub098\uc5d0\uac8c \uccab \ub208\uc5d0 \ubc18\ud55c \uc54c\ub9c8\ube44\ubc14 \ubc31\uc791\uc740 \uc138\ube44\uc57c\uae4c\uc9c0 \uadf8\ub140\ub97c \ub530\ub77c\uc628\ub2e4. \ud558\uc9c0\ub9cc \ub85c\uc9c0\ub098\uc5d0\uac8c\ub294 \ub3c8 \ubc14\ub974\ud1a8\ub85c\ub77c\ub294 \ud6c4\uacac\uc778\uc774 \uc788\uc5b4 \uc811\uadfc\uc774 \uc27d\uc9c0\uac00 \uc54a\uace0, \ub9e4\uc77c \ubc24 \ub85c\uc9c0\ub098\uc758 \uc9d1 \ucc3d\ubb38 \ubc11\uc5d0\uc11c \uc138\ub808\ub098\ub370\ub97c \ubd80\ub974\uba70 \uad6c\uc560\ub97c \ud574\ubcf4\uc9c0\ub9cc \uc2e0\ud1b5\uce58 \uc54a\ub2e4. \uadf8\ub54c, \uc54c\ub9c8\ube44\ubc14\uc758 \uc61b \ud558\uc778\uc774\uc5c8\ub358 \ud53c\uac00\ub85c\uac00 \ub4f1\uc7a5\ud558\uace0, \uc54c\ub9c8\ube44\ubc14\ub294 \uadf8\uc758 \ub3c4\uc6c0\uc744 \ubc1b\uc544 \ub9b0\ub3c4\ub85c\ub77c\ub294 \uac00\uba85\uc744 \uc0ac\uc6a9\ud574 \uadf8\ub140\uc758 \ud658\uc2ec\uc744 \uc0ac\ub294\ub370 \uc131\uacf5\ud558\uac8c \ub41c\ub2e4. \ud55c\ud3b8 \ub85c\uc9c0\ub098\uc758 \ubbf8\ubaa8\uc640 \uc7ac\uc0b0\uc744 \ub178\ub9ac\ub294 \ub3c8 \ubc14\ub974\ud1a8\ub85c\ub294 \ub3c8 \ubc14\uc2e4\ub9ac\uc624\uc640 \ud568\uaed8 \uadf8\ub140\uc640 \uacb0\ud63c\ud558\uae30 \uc704\ud55c \uc74c\ubaa8\ub97c \uafb8\ubbf8\uace0, \uc54c\ub9c8\ube44\ubc14 \ub610\ud55c \uadf8\ub140\ub97c \ub9cc\ub098\uae30 \uc704\ud574 \ud55c \ubc88\uc740 \uad70\uc778\uc73c\ub85c, \ud55c \ubc88\uc740 \ub3c8 \ubc14\uc2e4\ub9ac\uc624\uc758 \uc81c\uc790\ub85c \uc704\uc7a5\ud558\uc5ec \ub85c\uc9c0\ub098 \uc9d1\uc5d0 \uce68\uc785\ud558\uc9c0\ub9cc \ubc88\ubc88\ud788 \uc2e4\ud328\ud558\uac8c \ub41c\ub2e4. \ub3c8 \ubc14\ub974\ud1a8\ub85c\uc758 \uc74c\ubaa8\ub85c \uc778\ud574 \ub85c\uc9c0\ub098\ub294 \uc54c\ub9c8\ube44\ubc14\ub97c \uc758\uc2ec\ud558\uac8c \ub418\uc9c