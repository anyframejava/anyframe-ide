INSERT INTO MOVIES VALUES('MV-00129','\uc816\uc740 \uc785\uc220','.0','','posters/20140325_38_1395709705157E8wJd_JPEG_movie_image.jpg','','','2014-04-02',75,'','\ubaa9\uc7ac\uc0c1\uc5d0\uc11c \uc77c\ud558\ub294 \uac00\ub124\uc624\ub294 \uc0ac\uc7a5\uc758 \ub538 \uc0ac\uce58\ucf54\uc640 \uc560\uc778 \uc0ac\uc774\uc9c0\ub9cc, \uadf8\ub294 \uc810\uc810 \uc0ac\uce58\ucf54\uc5d0\uac8c \uc5bd\ub9e4\uc774\uae30 \uc2eb\uc5b4\uc9c4\ub2e4. \ub0b4\ud0a4\uc9c0 \uc54a\ub294 \ub370\uc774\ud2b8 \ud6c4, \uac00\ub124\uc624\ub294 \ucf5c\uac78\uc744 \ubd88\ub7ec \uae30\ubd84\uc804\ud658\uc744 \ud558\ub824 \ud55c\ub2e4. \uac00\ub124\uc624\ub294 \uc2a4\ub0b5\ubc14\uc5d0 \ub098\ud0c0\ub09c \uc694\ucf54\ub97c \ubcf4\uc790 \ud55c\ub208\uc5d0 \ubc18\ud558\ub294\ub370, \uadf8\ub294 \uc790\uc2e0\uc5d0\uac8c \ub9e4\uc815\ud55c \uc694\ucf54\uc5d0\uac8c \ub354\uc6b1 \ube60\uc838\ub4e0\ub2e4. \uac00\ub124\uc624\ub294 \uc0ac\uce58\ucf54\ub97c \uc18d\uc774\uace0 \ubaa9\uc7ac\uc0c1\uc5d0\uc11c \ub3c4\ub9dd\uccd0 \ub098\uc640 \uc694\ucf54\uc758 \uc544\ud30c\ud2b8\uc5d0\uc11c \ub458\ub9cc\uc758 \uc2dc\uac04\uc744 \ubcf4\ub0b4\ub294\ub370, \uac11\uc790\uae30 \ub610 \ub2e4\ub978 \ub0a8\uc790\uac00 \ucc3e\uc544\uc628\ub2e4. \uad6c\ub9c8\uc2dc\ub85c\uac00 \uc601\ud654\uac10\ub3c5\uc73c\ub85c\uc11c\uc758 \uc7ac\ub2a5\uc744 \ubcf4\uc774\uae30 \uc2dc\uc791\ud55c \ucd08\uae30\uc791. \uc774\ud6c4 \ub85c\ub9dd \ud3ec\ub974\ub178 \uc7a5\ub974\uc758 \ub300\ud45c \uac78\uc791\ub4e4\uc774 \uc904\uc904\uc774 \ud0c4\uc0dd\ud55c\ub2e4.','\ucfe0\ub9c8\uc2dc\ub85c \ud0c0\uce20\ubbf8','\uc5d0\uc790\uc640 \ubaa8\uc5d0\ucf54, \uc544\uc774\uce74\uc640 \ucf00\uc774\ucf54')
INSERT INTO MOVIES VALUES('MV-00130','\ubaa8\ub3c4\ub9ac\uac15','6.67','','posters/20140325_188_1395710010260oWjKg_JPEG_movie_image.jpg','','','2014-03-23',137,'','\ub2e4\uc774\uc1fc \uc2dc\ub300, \uc7ac\uc8fc\ub97c \ud3bc\uce58\uc9c0 \ubabb\ud55c \uc2dc\uc778 \uc18c\ub178\ub2e4 \uac00\ucfe0\uc694\ub294 \ud558\ub294 \uc77c \uc5c6\uc774 \uc2dc\uac04\uc744 \ubcf4\ub0b8\ub2e4. \uac00\ucfe0\uc694\ub294 \ubcd1\ub4e0 \uc544\ub0b4\ub97c \ub3cc\ubcfc \uc0dd\uac01\uc740\ucee4\ub155 \uc624\ub298\ub3c4 \uc0ac\ucc3d\uac00\ub97c \ucc3e\uc544\uac04\ub2e4. \uadf8\ub294 \uc2a4\uc2b9\uc758 \uc544\ub0b4 \uace0\ud1a0\uc5d0\uc640 \ubd88\ub95c\uad00\uacc4\ub97c \ub9fa\uace0, \ud568\uaed8 \ub3c4\ub9dd\uac08 \uacc4\ud68d\uae4c\uc9c0 \uc138\uc6b0\ub294\ub370, \uacc4\ud68d\uc744 \uc2e4\ud589\ud558\uae30 \uc9c1\uc804 \uac04\ud1b5\uc8c4\ub85c \uccb4\ud3ec\ub41c\ub2e4. \uc774\ud6c4 \uac00\ucfe0\uc694\ub294 \uc790\uc2e0\uc758 \uc5ec\uc131\ud3b8\ub825\uc744 \uc8fc\uc81c\ub85c \uc2dc\uc9d1\uc744 \ucd9c\ud310\ud574 \uc131\uacf5\ud558\uae30\uc5d0 \uc774\ub978\ub2e4. 1983\ub144 \uce78\uc601\ud654\uc81c\uc5d0\uc11c \uc18c\uac1c\ub418\uae30\ub3c4 \ud588\ub358 \uad6c\ub9c8\uc2dc\ub85c\uc758 \ud6c4\uae30 \ub300\ud45c\uc791\uc73c\ub85c \ub80c\uc870 \ubbf8\ud0a4\ud788\ucf54\uc758 \uc744 \uc601\ud654\ud654\ud588\ub2e4. \ub2e4\uc774\uc1fc \ub0ad\ub9cc\uc8fc\uc758\uc2dc\ub300\uc758 \uc2dc\uc778\uc740 \ubd84\ubc29\ud55c \uc0b6\uc5d0\uc11c \uc791\uac00\uc801 \uc591\ubd84\uc744 \uc5bb\uc73c\ub824 \ud55c\ub2e4.','\ucfe0\ub9c8\uc2dc\ub85c \ud0c0\uce20\ubbf8','\ud6c4\uc9c0 \ub9c8\ub9ac\ucf54, \ud558\uae30\uc640\ub77c \ucf04\uc774\uce58')
INSERT INTO MOVIES VALUES('MV-00131','\ubabd\ub465\uc774\uc758 \uc2ac\ud514','.0','','posters/20140319_276_1395192742648ylQ4P_JPEG_movie_image.jpg','','','2014-04-01',120,'','\uc624\uc624\ubb34\ub77c \uc870\uc9c1\uc758 \uc80a\uc740 \ud53c \ub2e4\ub098\uce74\ub294 \ub450\ubaa9\uacfc \uc0ac\uc774\uac00 \ud2c0\uc5b4\uc9c4 \ud6c4 \uacc4\uc18d\ud574\uc11c \uc704\ud5d8\ud55c \uc77c\uc744 \ub9e1\uac8c \ub41c\ub2e4. \uc2ec\uc9c0\uc5b4 \ub2e4\ub098\uce74\ub294 \uc870\uc9c1\uc744 \uc704\ud574 8\ub144 \ub3d9\uc548 \uc625\uc0b4\uc774\uae4c\uc9c0 \ud558\ub294\ub370, \uc870\uc9c1\uc740 \uadf8\ub97c \uc804\ud600 \ubc30\ub824\ud558\uc9c0 \uc54a\ub294\ub2e4. \uadf8\ub7ec\ub358 \uc911 \ub450\ubaa9\uc774 \uc4f0\ub7ec\uc9c0\uace0, \ub77c\uc774\ubc8c \uc624\uc624\uac00\uc640 \uc870\uc9c1\uc5d0\uc11c \ud638\uc2dc\ud0d0\ud0d0 \uc624\uc624\ubb34\ub77c \uc870\uc9c1\uc744 \ub3c4\ubc1c\ud55c\ub2e4. \ub2e4\ub098\uce74 \uc5ed\uc2dc \uc560\uc778 \uc544\uc720\ubbf8\uc758 \uc9d1\uc5d0\uc11c \ub450\ubaa9\uc5d0 \ub300\ud55c \ubcf5\uc218\ub97c \uacc4\ud68d\ud55c\ub2e4. \uad6c\ub9c8\uc2dc\ub85c\uac00 6\ub144 \ub9cc\uc5d0 \ubc1c\ud45c\ud55c \uc791\ud488\uc73c\ub85c, \uae30\ud0c0\uac00\ud0c0 \uac90\uc870\uc758 \ub3d9\uba85\uc18c\uc124\uc744 \uc601\ud654\ud654\ud588\ub2e4. \uc790\uc2e0\uc740 \ud55c\ub0b1 \ubabd\ub465\uc774\ucc98\ub7fc \ud558\ucc2e\uac8c \ud0dc\uc5b4\ub098 \uadf8\ub807\uac8c \uc0b4\ub2e4 \ub488\uc9c8 \uac83\uc774\ub77c\uace0 \uc774\uc57c\uae30\ud558\ub294 \ud55c \uc57c\ucfe0\uc790\uc758 \uc0b6\uc744 \uadf8\ub9b0 \ub4dc\ub77c\ub9c8. \u300c\ud0a4\ub124\ub9c8 \uc900\ubcf4\u300d\uc5d0\uc11c \uc120\uc815\ud55c \u20181994\ub144 \uc77c\ubcf8\uc601\ud654 \ubca0\uc2a4\ud2b8 10\u2019\uc5d0\uc11c 4\uc704\uc5d0 \uc120\uc815\ub418\uc5c8\ub2e4.','\ucfe0\ub9c8\uc2dc\ub85c \ud0c0\uce20\ubbf8','\uc624\ucfe0\ub2e4 \uc5d0\uc774\uc9c0, \ub098\uac00\uc2dc\ub9c8 \uc5d0\uc774\ucf54')
INSERT INTO MOVIES VALUES('MV-00132','\uc624\uae30\uc640 \uc545\ub3d9\ub4e4 \ub354 \ubb34\ube44','7.14','7.78','posters/20140415_249_1397527533064XO1be_JPEG_movie_image.jpg','\uc804\uccb4 \uad00\ub78c\uac00','\uc560\ub2c8\uba54\uc774\uc158','2014-04-17',80,'','Ep 1. \uc544\uc8fc \uba3c \uc61b\ub0a0, \uc6d0\uc2dc\uc2dc\ub300\uc5d0 \uc0b4\uace0 \uc788\ub294 \uace0\uc591\uc774 \u2018\uc624\uae30\u2019\ub294 \uc2e0\uc131\uc2dc \uc5ec\uae30\ub294 \u2018\ubd88\u2019\uc744 \ub2e4\uc2dc \ub418\ucc3e\uae30 \uc704\ud574 \ud654\uc0b0\uc73c\ub85c \ud5a5\ud558\uace0 \uadf8\uacf3\uc5d0\uc11c \uc0ac\ub791\uc2a4\ub7f0 \uace0\uc591\uc774 \u2018\uc62c\ub9ac\ube44\uc544\u2019\ub97c \ub9cc\ub09c\ub2e4. \ud558\uc9c0\ub9cc \u2018\uc624\uae30\u2019\ub97c \ud638\uc2dc\ud0d0\ud0d0 \ub178\ub9ac\ub294 \uc545\ub3d9 \uc0bc\ucd1d\uc0ac\uac00 \uc774\ub97c \uac00\ub9cc\ud788 \ub450\uace0 \ubcfc \ub9ac \uc5c6\ub2e4! Ep 2. \ud3c9\ud654\ub85c\uc6b4 \uc911\uc138\uc2dc\ub300, \uc790\uc720\ub85c\uc6b4 \uc601\ud63c \u2018\uc624\uae30\u2019 \uc655\uc790\uc5d0\uac8c\ub3c4 \uc0ac\ub791\uc774 \ucc3e\uc544\uc624\uc9c0\ub9cc \uc545\ub3d9 \uc0bc\ucd1d\uc0ac\uc758 \uacc4\ud68d\uc73c\ub85c \uc704\ud5d8\uc5d0 \ube60\uc9c4 \u2018\uc62c\ub9ac\ube44\uc544\u2019. \uc655\uc790\ub294 \uadf8\ub140\ub97c \uad6c\ud558\uae30 \uc704\ud574, \ub2a0\ub984\ud55c \uc9c4\uc9dc \uc0ac\ub098\uc774\ub85c \uac70\ub4ed\ub0a0 \uac83\uc744 \uacb0\uc2ec\ud55c\ub2e4! Ep 3. \uace0\uc591\uc774 \u2018\uc624\uae30\u2019, \uc15c\ub85d\ud648\uc988\uc758 \uc601\uc6d0\ud55c \ud30c\ud2b8\ub108 \uc653\uc2a8 \ubc15\uc0ac\uac00 \ub418\ub2e4! \u2018\uc624\uae30 \uc653\uc2a8\u2019\uc740 \u2018\uc62c\ub9ac\ube44\uc544\u2019\uc758 \uc783\uc5b4\ubc84\ub9b0 \ud669\uae08 \uc5f4\uc1e0\ub97c \ucc3e\uae30\uc5d0 \ub098\uc120\ub2e4. \ud558\uc9c0\ub9cc \uc545\ub3d9 \uc0bc\ucd1d\uc0ac\uc758 \uacc4\ub7b5\uc73c\ub85c \uc624\ud788\ub824 \ub3c4\ub451\uc73c\ub85c \ubab0\ub9ac\ub294 \uc704\uae30\ub97c \ub9de\ub294\ub370\u2026 Ep 4. \uc2a4\ud0c0\uc6cc\uc988\uc758 \uad11\u