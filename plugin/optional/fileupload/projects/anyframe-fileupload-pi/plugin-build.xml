<?xml version="1.0" encoding="UTF-8" ?>
<plugin name="fileupload" description="FileUpload plugin consists of sample code that provides functionality uploading binary-file using the Commons Fileupload and jQuery.">
  <groupId>org.anyframe.plugin</groupId>
  <artifactId>anyframe-fileupload-pi</artifactId>
  <version>1.2.1-SNAPSHOT</version>
  <dependent-plugins>
    <dependent-plugin name="core" version="1.6.1-SNAPSHOT"/>
  </dependent-plugins>
  <resources>
    <resource dir="src/main/java" filtered="true" packaged="true">
      <include name="**/fileupload/**/*.java"/>
    </resource>
    <resource dir="src/main/resources" filtered="true" packaged="false">
      <include name="**/*fileupload*.xml"/>
      <include name="**/*fileupload*.properties"/>
    </resource>
    <resource dir="src/test/java" filtered="true" packaged="true">
      <include name="**/fileupload/**/*.java"/>
    </resource>
    <resource dir="src/test/resources" filtered="true" packaged="false">
      <include name="**/*fileupload*.xml"/>
    </resource>
    <resource dir="src/main/webapp" filtered="false" packaged="false">
      <include name="**/fileupload/**/*.*"/>
      <exclude name="**/anyframe.jsp" merged="true"/>
    </resource>
  </resources>
  <samples>true</samples>
  <essential>false</essential>
  <checked>false</checked>
</plugin>