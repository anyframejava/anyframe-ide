<?xml version="1.0"?>

<project name="Anyframe Common Path" >	
	
	<property name = "anyframe.home" value = "${anyframeHome}"/>    
	<property name = "anyframe.repo" value = "${anyframe.home}/repo"/>    
		
	<path id="anyframe.antpath">
		<fileset dir="${anyframe.home}/ide/cli/lib" includes="**/*.jar" />
	</path>
	<path id="anyframe.reportpath">
		<fileset dir="${basedir}/.settings/anyframe/inspection" includes="**/*.jar" excludes = "**/*-sources.jar"/>
	</path>

</project>


